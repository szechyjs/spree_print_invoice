<% if @order and @order.completed_at? %>
  <% buttons = Spree::PrintInvoice::Config[:print_buttons]
     buttons = buttons.split(",").collect{|b| b.strip } %>
  <%  buttons.each do |button| %>
    <% icon = button == "packaging_slip" ? "icon-truck" : "icon-money" %>
    <li><%= button_link_to(Spree.t(button.to_s + "_print"), spree.admin_order_path(@order, :pdf , :template => button), :icon => icon) %></li>
  <% end %>
<% end %>  
